def type_checker(input_value):
    return type(input_value)

# Example usage
print(type_checker(42))          # Output: <class 'int'>
print(type_checker("Hello"))     # Output: <class 'str'>
print(type_checker([1, 2, 3]))   # Output: <class 'list'>
print(type_checker(3.14))        # Output: <class 'float'>
